
    <%= form_for [@artist, @song] do |f| %>

      <div class="form-group">
        <p><%= f.label :title %></p>
        <p><%= f.text_field :title %></p>
      </div>

      <div class="form-group">
        <p><%= f.label :Genre %></p>
        <p><%= f.text_field :genre %></p>
      </div>


      <div class="actions">
        <p><%= f.submit "Create" %></p>
      </div>
    <%end%>
