<h1><%= @song.title %></h1>

  <p>Genre: <%= @song.genre %></p>
  <p>Artist: <%= @song.artist.name %></p>
  <p>About the artist: <%= @song.artist.bio %></p>





<%= link_to "Edit", edit_song_path(@song.id), class: " btn btn-default" %>
<%= link_to "Delete", song_path(@song.id), class: " btn btn-default", method: :delete, data: { confirm: "Are you sure?" } %>
